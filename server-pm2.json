{
    "apps": [{
        "name": "StdAux Website",
        "args": ["run", "dev"],
        "exec_mode": "fork",
        "instances": "1",
        "script": "npm",
        "wait_ready": true,
        "autorestart": false,
        "watch": true,
        "error_file": "log/frontend/error/error.log",
        "out_file": "log/frontend/output/output.log",
        "log_file": "log/frontend/combined/combined.log",
        "max_restarts": 5,
        "restart_delay": 4000
    }]
}

